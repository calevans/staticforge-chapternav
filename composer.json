{
    "name": "calevans/staticforge-chapternav",
    "description": "StaticForge Feature: ChapterNav",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EICC\\StaticForge\\Features\\ChapterNav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EICC\\StaticForge\\Features\\ChapterNav\\Tests\\": "tests/"
        }
