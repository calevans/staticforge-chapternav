    "require": {
        "php": "^8.4",
        "eicc/utils": "^1.2",
        "eicc/staticforge": ">=1.14.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "staticforge": {
            "feature": "EICC\\StaticForge\\Features\\ChapterNav\\Feature"
        }
    }
}